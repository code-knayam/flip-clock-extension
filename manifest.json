{
	"manifest_version": 3,
	"name": "Flip Clock",
	"version": "1.3",
	"description": "A minimalistic flip clock on your new tab page.",
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"permissions": [
		"storage",
		"audio",
		"alarms",
		"tabs",
		"notifications",
		"offscreen"
	],
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "icons/icon.png",
		"48": "icons/icon.png",
		"128": "icons/icon.png"
	},
	"author": "Mayank Jain",
	"web_accessible_resources": [{
		"resources": [
			"assets/chime.mp3",
			"offscreen.html",
			"assets/offscreen.js"
		],
		"matches": ["<all_urls>"]
	}]
}